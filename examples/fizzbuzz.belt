// FizzBuzz logic
idhar aao
    ye sambhal n = 1;
    jab tak main na bolu (n <= 20) {
        agar sharam hai (n % 15 == 0) {
            jawab de "FizzBuzz";
        } nahi to (n % 3 == 0) {
            jawab de "Fizz";
        } nahi to (n % 5 == 0) {
            jawab de "Buzz";
        } warna belt {
            jawab de n;
        }
        n += 1;
    }
jao padhai karo
