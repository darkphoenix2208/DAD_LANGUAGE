{
    "name": "dad-lang-vscode",
    "displayName": "Dad Lang",
    "description": "Language support for Dad Lang",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dadlang",
                "aliases": [
                    "Dad Lang",
                    "dadlang",
                    "dad",
                    "belt"
                ],
                "extensions": [
                    ".dad",
                    ".belt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dadlang",
                "scopeName": "source.dadlang",
                "path": "./dadlang.tmLanguage.json"
            }
        ]
    }
}